{
  "name": "@tiagoantunespt/y-supabase",
  "version": "0.1.0",
  "description": "Supabase Realtime provider for Yjs",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "example:monaco": "npx vite examples/monaco"
  },
  "keywords": [
    "yjs",
    "supabase",
    "realtime",
    "collaboration"
  ],
  "author": "Tiago Antunes <tiagoantunespt@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiagoantunespt/y-supabase.git"
  },
  "bugs": {
    "url": "https://github.com/tiagoantunespt/y-supabase/issues"
  },
  "homepage": "https://github.com/tiagoantunespt/y-supabase#readme",
  "peerDependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "yjs": "^13.0.0"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "y-protocols": "^1.0.7"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "monaco-editor": "^0.55.1",
    "tsdown": "^0.19.0-beta.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "y-monaco": "^0.1.6",
    "yjs": "^13.6.29"
  }
}
